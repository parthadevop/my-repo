pipeline {
    agent any

    stages {
        stage('Stage 1') {
            steps {
                sh 'exit 1'
            }
        }
        stage('Stage 2') {
            when {
                not {
                    previousBuild().result == 'FAILURE'
                }
            }
            steps {
                echo 'Hello World'
            }
        }
    }
}
