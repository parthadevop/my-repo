pipeline{
    agent any
    
    stages{
        stage('build'){
            steps{
                sh 'echo Build is successful'
            }
        }
        stage('test'){
            steps{
                sh 'echo test is successfull'
            }
        }
        stage('deploy'){
            steps{
                sh 'echo deploy is successfull'
            }
        }

}

}
